{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "collectionName": "device",
  "required": ["phone", "tablet", "pager"],
  "properties": {
    "phone": {
      "$id": "#/properties/phone",
      "type": "object",
      "title": "The Phone Schema",
      "required": ["Pei", "gpsi", "supi", "datetime"],
      "properties": {
        "Pei": {
          "$id": "#/properties/phone/properties/Pei",
          "type": "string",
          "title": "The Pei Schema",
          "default": "",
          "examples": ["5/8C"],
          "pattern": "^(.*)$"
        },
        "gpsi": {
          "$id": "#/properties/phone/properties/gpsi",
          "type": "string",
          "title": "The Gpsi Schema",
          "default": "",
          "examples": ["mEW"],
          "pattern": "^(.*)$"
        },
        "supi": {
          "$id": "#/properties/phone/properties/supi",
          "type": "string",
          "title": "The Supi Schema",
          "default": "",
          "examples": ["imsi-549420"],
          "pattern": "^(.*)$"
        },
        "datetime": {
          "$id": "#/properties/phone/properties/datetime",
          "type": "string",
          "title": "The Datetime Schema",
          "default": "",
          "examples": ["2019-61-97 00:71:37"],
          "pattern": "^(.*)$"
        }
      }
    },
    "tablet": {
      "$id": "#/properties/tablet",
      "type": "object",
      "title": "The Tablet Schema",
      "required": ["Pei", "gpsi", "supi", "datetime"],
      "properties": {
        "Pei": {
          "$id": "#/properties/tablet/properties/Pei",
          "type": "string",
          "title": "The Pei Schema",
          "default": "",
          "examples": ["FU:Dp%rC"],
          "pattern": "^(.*)$"
        },
        "gpsi": {
          "$id": "#/properties/tablet/properties/gpsi",
          "type": "string",
          "title": "The Gpsi Schema",
          "default": "",
          "examples": ["k5_a@"],
          "pattern": "^(.*)$"
        },
        "supi": {
          "$id": "#/properties/tablet/properties/supi",
          "type": "string",
          "title": "The Supi Schema",
          "default": "",
          "examples": ["nai-]M:].4Tsk"],
          "pattern": "^(.*)$"
        },
        "datetime": {
          "$id": "#/properties/tablet/properties/datetime",
          "type": "string",
          "title": "The Datetime Schema",
          "default": "",
          "examples": ["1084-84-58 73:19:82"],
          "pattern": "^(.*)$"
        }
      }
    },
    "pager": {
      "$id": "#/properties/pager",
      "type": "object",
      "title": "The Pager Schema",
      "required": ["Pei", "gpsi", "supi", "datetime"],
      "properties": {
        "Pei": {
          "$id": "#/properties/pager/properties/Pei",
          "type": "string",
          "title": "The Pei Schema",
          "default": "",
          "examples": ["imeisv-1874052444523531"],
          "pattern": "^(.*)$"
        },
        "gpsi": {
          "$id": "#/properties/pager/properties/gpsi",
          "type": "string",
          "title": "The Gpsi Schema",
          "default": "",
          "examples": ["msisdn-97684828405"],
          "pattern": "^(.*)$"
        },
        "supi": {
          "$id": "#/properties/pager/properties/supi",
          "type": "string",
          "title": "The Supi Schema",
          "default": "",
          "examples": ["nai-x`e1_U&2>"],
          "pattern": "^(.*)$"
        },
        "datetime": {
          "$id": "#/properties/pager/properties/datetime",
          "type": "string",
          "title": "The Datetime Schema",
          "default": "",
          "examples": ["3442-01-02 11:76:70"],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}
