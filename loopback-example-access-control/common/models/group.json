{
  "name": "Group",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "descr": {
      "type": "string",
      "default": "describe"
    },
    "name": {
      "type": "string",
      "unique": true
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasAndBelongsToMany",
      "model": "User",
      "foreignKey": "userId",
      "through": "UserGroup"
    },
    "permissions": {
      "type": "hasAndBelongsToMany",
      "model": "Permission",
      "foreignKey": "permissionId",
      "through": "GroupPermission"
    }
  },
  "acls": [],
  "methods": {}
}
