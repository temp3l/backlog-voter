{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "userName": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "authSource": {
      "type": "string"
    },
    "expiry": {
      "type": "date"
    },
    "locked": {
      "type": "boolean"
    },
    "lastLogin": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "projects": {
      "type": "hasMany",
      "model": "project",
      "foreignKey": "ownerId"
    },
    "teams": {
      "type": "hasMany",
      "model": "team",
      "foreignKey": "ownerId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId"
    },
    "reports": {
      "type": "hasMany",
      "model": "Report",
      "foreignKey": "ownerId"
    },
    "groups": {
      "type": "hasAndBelongsToMany",
      "model": "Group",
      "foreignKey": "groupId",
      "through": "UserGroup"
    },
    "activities": {
      "type": "hasAndBelongsToMany",
      "model": "Activity",
      "foreignKey": "activityId",
      "through": "UserActivity"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": ["find"]
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "__create__reports",
        "__get__reports",
        "__destroyById__accessTokens",
        "getRolesById",
        "info",
        "__get__groups",
        "find",
        "findById"
      ]
    }
  ],
  "methods": {}
}
